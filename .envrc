# Journal repository environment configuration
# This ensures gh CLI always uses the correct account (verlyn13)

# Set the GitHub host to use the correct account
export GH_HOST=github.com

# Ensure we're using the verlyn13 account
export GITHUB_USER=verlyn13

# Optional: Set default branch for this repo
export DEFAULT_BRANCH=main

# Note: Since verlyn13 is already the active account on github.com,
# gh commands will work correctly in this repo